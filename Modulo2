Sub AlterarPessoa()
    Call LeftClick(90, 68) 'Buscando a tela de Pessoas
    Call LeftClick(287, 129)
    SendKeys ("{F11}")
    SendKeys ("#pessoa5") 'filtrando a pessoa que vai ser alterada
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{F3}") 'editando a pessoa 5
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call LeftClick(675, 285)
    Call LeftClick(675, 285) 'acessando a guia Complemento
    Call LeftClick(704, 306) 'acessando a subguia Contatos
    
    SendKeys ("6")
    SendKeys ("{ENTER 4}")
    SendKeys ("email_de_teste@realtec.com")
    SendKeys ("{ENTER 3}")
    
    Call LeftClick(602, 352)
    SendKeys ("3")
    SendKeys ("{ENTER 2}")
    SendKeys ("Pessoa de teste")
    SendKeys ("{ENTER}")
    SendKeys ("(34)99999-1234")
    SendKeys ("{ENTER 3}")
    
    Application.Wait (Now + TimeValue("0:00:01"))
    Call RightClick(652, 449)
    SendKeys ("T")
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(602, 766) 'salvando os dados
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys "{ESC}" 'fechando a tela
    
    '--- verificação de dados
        Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("A2").Value <> "(34)99999-1234") Then
            MsgBox "Ocorreu algum erro com o Contato" & vbCrLf & "Valor esperado: (34)99999-1234" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A2").Value
            End
        End If
        If (Sheets(2).Range("B2").Value <> "Pessoa de Teste") Then
            MsgBox "Ocorreu algum erro com o nome do Contato" & vbCrLf & "Valor esperado: Pessoa de Teste" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B2").Value
            End
        End If
        If (Sheets(2).Range("C2").Value <> "Celular") Then
            MsgBox "Ocorreu algum erro com o Tipo de Contato" & vbCrLf & "Valor esperado: Celular" & vbCrLf & "Valor obtido:" & Sheets(2).Range("C2").Value
            End
        End If
        If (Sheets(2).Range("D2").Value <> "0") Then
            MsgBox "Ocorreu algum erro com o Enviar Email do Contato" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D2").Value
            End
        End If
        '----Verificação da outra Unidade

        If (Sheets(2).Range("A3").Value <> "email_de_teste@realtec.com") Then
            MsgBox "Ocorreu algum erro com o Contato" & vbCrLf & "Valor esperado: email_de_teste@realtec.com" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A3").Value
            End
        End If
        If (Sheets(2).Range("B3").Value <> "") Then
            MsgBox "Ocorreu algum erro com o nome do Contato" & vbCrLf & "Valor esperado era pra ser vazio: " & vbCrLf & "Valor obtido:" & Sheets(2).Range("B3").Value
            End
        End If
        If (Sheets(2).Range("C3").Value <> "E-mail") Then
            MsgBox "Ocorreu algum erro com o Tipo de Contato" & vbCrLf & "Valor esperado: E-mail" & vbCrLf & "Valor obtido:" & Sheets(2).Range("C3").Value
            End
        End If
        If (Sheets(2).Range("D3").Value <> "0") Then
            MsgBox "Ocorreu algum erro com o Enviar Email do Contato" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D3").Value
            End
        End If

    '---- fim da verificação de dados
    
    '---- limpando os dados
        Sheets(2).Range("A1:Z12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
End Sub


Sub CadastrarFormaPagamento()
    Call LeftClick(209, 66) 'Buscando a tela de Forma Pagamento
    Call LeftClick(1004, 118)
    Call LeftClick(1034, 218)
    SendKeys ("{F2}") 'abrindo a tela de Forma Pagamento
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'primeira forma de pagamento
        SendKeys ("2x S/E")
        SendKeys ("{ENTER}")
        SendKeys ("{DOWN}") 'informando que é crediário
        SendKeys ("{ENTER}")
        SendKeys ("2") 'informando que qtd parcelas
        SendKeys ("{ENTER}")
        SendKeys ("{DOWN}") 'infromando se é entrada
        SendKeys ("{ENTER}")
        Call LeftClick(743, 655)
        Call LeftClick(743, 655) 'salvando os dados
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys ("{F2}") 'abrindo a tela de Forma Pagamento
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'segunda forma de pagamento
        SendKeys ("1x S/E")
        SendKeys ("{ENTER}")
        SendKeys ("{DOWN}") 'informando que é crediário
        SendKeys ("{ENTER}")
        SendKeys ("1") 'informando que qtd parcelas
        SendKeys ("{ENTER}")
        SendKeys ("{DOWN}") 'infromando se é entrada
        SendKeys ("{ENTER}")
        Call LeftClick(743, 655)
        Call LeftClick(743, 655) 'salvando os dados
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys ("{F2}") 'abrindo a tela de Forma Pagamento
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'terceira forma de pagamento
        SendKeys ("3x S/E")
        SendKeys ("{ENTER}")
        SendKeys ("{DOWN}") 'informando que é crediário
        SendKeys ("{ENTER}")
        SendKeys ("3") 'informando que qtd parcelas
        SendKeys ("{ENTER}")
        SendKeys ("{DOWN}") 'infromando se é entrada
        SendKeys ("{ENTER}")
        Call LeftClick(743, 655)
        Call LeftClick(743, 655) 'salvando os dados
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys ("{F2}") 'abrindo a tela de Forma Pagamento
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'quarta forma de pagamento
        SendKeys ("2x c/E")
        SendKeys ("{ENTER}")
        SendKeys ("{DOWN}") 'informando que é crediário
        SendKeys ("{ENTER}")
        SendKeys ("2") 'informando que qtd parcelas
        Call LeftClick(743, 655)
        Call LeftClick(743, 655) 'salvando os dados
    Application.Wait (Now + TimeValue("0:00:03"))
    
    Application.Wait (Now + TimeValue("0:00:03"))
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys "{ESC}" 'fechando a tela
    
    

    '--- verificação de dados
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("A2").Value <> "1x S/E") Then
            MsgBox "Ocorreu algum erro com a descrição da Forma de Pagamento" & vbCrLf & "Valor esperado: 1x S/E" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A2").Value
            End
        End If

        If (Sheets(2).Range("A3").Value <> "2x C/E") Then
            MsgBox "Ocorreu algum erro com a descrição da Forma de Pagamento" & vbCrLf & "Valor esperado: 2x C/E" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A3").Value
            End
        End If
        
        If (Sheets(2).Range("A4").Value <> "2x S/E") Then
            MsgBox "Ocorreu algum erro com a descrição da Forma de Pagamento" & vbCrLf & "Valor esperado: 2x S/E" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A4").Value
            End
        End If
        
        If (Sheets(2).Range("A5").Value <> "3x S/E") Then
            MsgBox "Ocorreu algum erro com a descrição da Forma de Pagamento" & vbCrLf & "Valor esperado: 3x S/E" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A5").Value
            End
        End If

        If (Sheets(2).Range("A6").Value <> "A Vista") Then
            MsgBox "Ocorreu algum erro com a descrição da Forma de Pagamento" & vbCrLf & "Valor esperado: A Vista" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A6").Value
            End
        End If

        Dim contador As Integer
        contador = 2
        Do While (contador < 6)
            If (Sheets(2).Range("C" & contador).Value <> "Crediário     ") Then
                MsgBox "Ocorreu algum erro com o Tipo da Forma de Pagamento" & vbCrLf & "Valor esperado: Crediário     " & vbCrLf & "Valor obtido:" & Sheets(2).Range("C" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
        
        If (Sheets(2).Range("C6").Value <> "A Vista       ") Then
            MsgBox "Ocorreu algum erro com o Tipo da Forma de Pagamento" & vbCrLf & "Valor esperado: A Vista       " & vbCrLf & "Valor obtido:" & Sheets(2).Range("C6").Value
            End
        End If
        
        contador = 2
        Do While (contador < 7)
            If (Sheets(2).Range("D" & contador).Value <> 0) Then
                MsgBox "Ocorreu algum erro com a diferença de dias da Forma de Pagamento" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
        
        If (Sheets(2).Range("F3").Value <> "S") Then
            MsgBox "Ocorreu algum erro com a Entrada da Forma de Pagamento" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F3").Value
            End
        End If
        
        If (Sheets(2).Range("F6").Value <> "S") Then
            MsgBox "Ocorreu algum erro com a Entrada da Forma de Pagamento" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F6").Value
            End
        End If
        
        If (Sheets(2).Range("F2").Value <> "N") Then
            MsgBox "Ocorreu algum erro com a Entrada da Forma de Pagamento" & vbCrLf & "Valor esperado: N" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If
        
        If (Sheets(2).Range("F5").Value <> "N") Then
            MsgBox "Ocorreu algum erro com a Entrada da Forma de Pagamento" & vbCrLf & "Valor esperado: N" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F5").Value
            End
        End If
        
    '---- fim da verificação de dados
    
    '---- limpando os dados
        Sheets(2).Range("A1:Z12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
    
End Sub



Sub EntradaCompra1()
    Call LeftClick(149, 66) 'Buscando a tela de Compras
    Call LeftClick(36, 125)
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(789, 693)
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys ("{F2}") 'Criando a entrada compra 1
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys ("31180304248801000121550000000597691000597698") 'Chave
    SendKeys ("{ENTER 6}")
    SendKeys ("1102") 'CFOP
    SendKeys ("{ENTER}")
    SendKeys ("3") 'Fornecedor
    SendKeys ("{ENTER 2}")
    SendKeys ("{UP 2}") 'Informa que é simples nacional
    SendKeys ("{ENTER}")
    SendKeys ("1") 'Funcionário
    SendKeys ("{ENTER}")
    SendKeys ("2") 'Forma de Pagamento
    SendKeys ("{ENTER}")
    SendKeys ("{UP 2}") 'selecionando a modalidade do frete
    SendKeys ("{ENTER}")
    Call LeftClick(1416, 335)
    Application.Wait (Now + TimeValue("0:00:05"))
    Call LeftClick(1416, 335)
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(1416, 335)
    Call LeftClick(1416, 335)
    '----Primeiro item
        SendKeys ("1") 'codigo do produto
        SendKeys ("{ENTER 2}")
        SendKeys ("103") 'cst
        SendKeys ("{ENTER}")
        SendKeys ("150") 'quantidade
        SendKeys ("{ENTER}")
        SendKeys ("2,5") 'unitário
        SendKeys ("{ENTER 3}")
        SendKeys ("21") '%ICMS
        SendKeys ("{ENTER 4}")
    Application.Wait (Now + TimeValue("0:00:01"))
    '----Segundo Item
        SendKeys ("2")
        SendKeys ("{ENTER 2}")
        SendKeys ("900") 'cst
        SendKeys ("{ENTER}")
        SendKeys ("100") 'quantidade
        SendKeys ("{ENTER}")
        SendKeys ("15") 'unitário
        SendKeys ("{ENTER 3}")
        SendKeys ("5")
        SendKeys ("{ENTER}")
        SendKeys ("2") '%ICMS
        SendKeys ("{ENTER 7}")
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call LeftClick(1032, 743) 'abrindo a tela de rateio
    SendKeys ("12,5") 'frete
    SendKeys ("{ENTER 2}")
    SendKeys ("7,5") 'seguro
    SendKeys ("{ENTER 2}")
    SendKeys ("5") 'despesa
    SendKeys ("{ENTER 2}")
    SendKeys ("{ESC}") 'fechando a tela de rateio
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call LeftClick(503, 837)   'clicando no estoque
    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(574, 841)   'clicando no financeiro
    Application.Wait (Now + TimeValue("0:00:03"))
    Call LeftClick(1236, 453)   'clicando no botão Gerar
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(670, 743)   'salvando o financeiro
    Application.Wait (Now + TimeValue("0:00:03"))
    
    
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(672, 403)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação dos produtos inseridos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("F2").Value <> "103") Then
            MsgBox "Ocorreu algum erro com o CST da Entrada Compra" & vbCrLf & "Valor esperado: 103" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If

        If (Sheets(2).Range("F3").Value <> "900") Then
            MsgBox "Ocorreu algum erro com o CST da Entrada Compra" & vbCrLf & "Valor esperado: 900" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F3").Value
            End
        End If
        
        If (Sheets(2).Range("H2").Value <> "150") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("H1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 150" & vbCrLf & "Valor obtido:" & Sheets(2).Range("H2").Value
            End
        End If
        
        If (Sheets(2).Range("H3").Value <> "100") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("H1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 100" & vbCrLf & "Valor obtido:" & Sheets(2).Range("H3").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("I2").Value) <> "2,5") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 2,5" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I2").Value
            End
        End If
        
        If (Sheets(2).Range("I3").Value <> "15") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 15" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I3").Value
            End
        End If
        
        If (Sheets(2).Range("K2").Value <> "375") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 375" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K2").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("K3").Value) <> "1500") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 1500" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K3").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("L2").Value) <> "0") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L2").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("L3").Value) <> "1425") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 1425" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L3").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("M2").Value) <> "0") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("M2").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("M3").Value) <> "171") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("M1").Value & " da Entrada Compra" & vbCrLf & "Valor esperado: 171" & vbCrLf & "Valor obtido:" & Sheets(2).Range("M3").Value
            End
        End If
        
        Sheets(2).Range("A1:Z12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
    'fim da verificação dos produtos inseridos
    
    
    SendKeys "{ESC}" 'fechando a tela de inclusao da Entrada compra
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys "{ESC}" 'fechando a tela de consulta da Entrada Compra
    Application.Wait (Now + TimeValue("0:00:01"))
End Sub



