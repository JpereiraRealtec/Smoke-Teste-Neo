Sub EntradaCompra3()
    Call LeftClick(149, 66) 'Buscando a tela de Importar XML
    Call LeftClick(89, 116)
    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(729, 220) 'abrindo a tela de importar XML local
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys ("\\TST-07\Setor de Teste\Utilitários\1 - Qualidade\Smoke Test\Neo\XML Smoke Test\31130611069897000126550010000310751005835454-nfe.xml") 'selecionando o XML
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{ENTER}")
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("S")
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys ("1403") 'CFOP
    SendKeys ("{ENTER}")
    SendKeys ("H") 'informando o dia atual no campo de Emissão
    SendKeys ("{ENTER 2}")
    SendKeys ("3") 'fornecedor
    SendKeys ("{ENTER}")
    SendKeys ("1") 'funcionário
    SendKeys ("{ENTER 2}")
    SendKeys ("3") 'forma de pagamento
    SendKeys ("{ENTER}")
    SendKeys ("{DOWN}")
    SendKeys ("{F4}") 'excluindo o item 2
    SendKeys ("S")
    SendKeys ("{F4}") 'excluindo o item 3
    SendKeys ("S")
    SendKeys ("{RIGHT 2}")
    SendKeys ("2") 'associando o #produto2
    SendKeys ("{ENTER}")
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys ("{F3}") 'editando o item
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys ("1403") 'cfop do item
    Call LeftClick(502, 606)
    Application.Wait (Now + TimeValue("0:00:03"))
    Call LeftClick(502, 606)
    Call LeftClick(502, 606) 'salvando o item
    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(502, 606)
    
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call LeftClick(1417, 814) 'exportando
    Call LeftClick(503, 742)
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("S") 'confirmando que vai exportar
    
    
    
    Application.Wait (Now + TimeValue("0:00:05"))
    Call RightClick(672, 403)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))

    '--- verificação dos produtos inseridos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("F2").Value <> "60") Then
            MsgBox "Ocorreu algum erro com o CFOP da Entrada Compra" & vbCrLf & "Valor esperado: 60" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If

        If (Sheets(2).Range("G2").Value <> "1403") And (Sheets(2).Range("G2").Value <> "1,403") Then
            MsgBox "Ocorreu algum erro com o CFOP da Entrada Compra" & vbCrLf & "Valor esperado: 1.403" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G2").Value
            End
        End If

        If (Sheets(2).Range("I2").Value <> "27") Then
            MsgBox "Ocorreu algum erro com o unitário da Entrada Compra" & vbCrLf & "Valor esperado: 27" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I2").Value
            End
        End If
    
        If (Sheets(2).Range("K2").Value <> "27") Then
            MsgBox "Ocorreu algum erro com o unitário da Entrada Compra" & vbCrLf & "Valor esperado: 27" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K2").Value
            End
        End If
        
        Sheets(2).Range("A1:Z12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
    'fim da verificação dos produtos inseridos
    
    Call LeftClick(503, 837)   'clicando no estoque
    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(574, 841)
    Application.Wait (Now + TimeValue("0:00:03"))
    Call LeftClick(570, 840)   'clicando no financeiro
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("S") 'Clicando no botão Sim
    Application.Wait (Now + TimeValue("0:00:03"))
    Call LeftClick(1236, 453)   'clicando no botão Gerar
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(670, 743)   'salvando o financeiro
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys "{ESC}" 'fechando a tela de inclusao da Entrada compra
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys "{ESC}" 'fechando a tela de XML
    Application.Wait (Now + TimeValue("0:00:01"))
End Sub



