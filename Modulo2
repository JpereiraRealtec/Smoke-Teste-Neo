Public Const MOUSEEVENTF_LEFTDOWN = &H2
Public Const MOUSEEVENTF_LEFTUP = &H4
Public Const MOUSEEVENTF_RIGHTDOWN As Long = &H8
Public Const MOUSEEVENTF_RIGHTUP As Long = &H10

Private dtmNext As Date



Sub teste()

    Dim posicao As Integer
    posicao = Sheets(3).Range("B1").Value
    
    Call LimparDados

    If (posicao <= 1) Then
        Call Atualizar("3,7", "1. Empresa")
        Call CriarEmpresa
    End If
    
    If (posicao <= 2) Then
        Call Atualizar("7,4", "2. Parametros")
        Call Parametrizacao
    End If
        
    If (posicao <= 4) Then
        Call Atualizar("11,11", "4.1 Cadastrar Usuário")
        Call CadastrarUsuário
    End If
    
    If (posicao <= 5) Then
        Call Atualizar("14,81", "4.2 Cadastrar Funcionário")
        Call CadastrarFuncionario
    End If
    
    If (posicao <= 6) Then
        Call Atualizar("18,51", "4.3 Cadastrar Município")
        Call CadastrarMunicipio
    End If
    
    If (posicao <= 7) Then
        Call Atualizar("22,22", "4.4 Conta Movimento")
        Call CadastrarContaMovimento
    End If
    
    If (posicao <= 8) Then
        Call Atualizar("25,92", "Cadastros Gerais")
        Call CadastrarClassificacao
        Call CadastrarMarca
        Call CadastrarGrupo
        Call CadastrarSubGrupo
        Call CadastrarUnidade
        Call CadastrarFormaPagamento
    End If

    If (posicao <= 9) Then
        Call Atualizar("29,62", "4.5.1 Cadastrar Pessoa 1")
        Call CadastrarPessoa(0, "34101057605", 1, 38800000, 3162104, 1, 1, "", "#pessoa1") 'tipo, documento, tipoIE, CEP, municipio, cliente, fornecedor, IE, nome
    End If
    
    If (posicao <= 10) Then
        Call Atualizar("33,33", "4.5.2 Cadastrar Pessoa 2")
        Call CadastrarPessoa(1, "04140757000131", 0, 75500000, 5200050, 1, 0, "107732041", "#pessoa2") 'tipo, documento, tipoIE, CEP, municipio, cliente, fornecedor, IE, nome
    End If
    
    If (posicao <= 11) Then
        Call Atualizar("37,03", "4.5.3 Cadastrar Pessoa 3")
        Call CadastrarPessoa(2, "57876780016", 1, "00000000", 5300109, 1, 1, "", "#pessoa3")        'tipo, documento, tipoIE, CEP, municipio, cliente, fornecedor, IE, nome
    End If
    
    If (posicao <= 12) Then
        Call Atualizar("40,74", "4.5.4 Cadastrar Pessoa 4")
        Call CadastrarPessoa(1, "02110065000115", 1, 38800000, 3162104, 0, 1, "", "#pessoa4") 'tipo, documento, tipoIE, CEP, municipio, cliente, fornecedor, IE, nome
    End If

    If (posicao <= 13) Then
        Call Atualizar("44,44", "4.6.1 Cadastrar Produto 1")
        Call CadastrarProduto(1, 0, 103, 21, "1000000000016", "#produto1", "3", "92079010") 'unidade, estoque, CST, AliqICMS, barras, nome, venda, NCM
    End If
    
    If (posicao <= 14) Then
        Call Atualizar("48,14", "4.6.2 Cadastrar Produto 2")
        Call CadastrarProduto(2, 0, 900, 2, "1000000000023", "#produto2", "20", "92079010") 'unidade, estoque, CST, AliqICMS, barras, nome, venda, NCM
    End If
    
    If (posicao <= 15) Then
        Call Atualizar("51,85", "4.6.3 Cadastrar Produto 3")
        Call CadastrarProduto(1, 2, 900, 2, "1000000000030", "#produto3", "30", "92079010") 'unidade, estoque, CST, AliqICMS, barras, nome, venda, NCM
    End If
    
    If (posicao <= 16) Then
        Call Atualizar("55,55", "4.7 Serviço")
        Call CadastrarGrupoServico
        Call CadastrarServiço(1, "#serviço1", "150") 'unidade, descrição, preço
    End If
    
    If (posicao <= 18) Then
        Call Atualizar("59,25", "5.1.1 Entrada Compra 1")
        Call EntradaCompra1
    End If
    
    If (posicao <= 19) Then
        Call Atualizar("62,96", "5.1.2 Entrada Compra 2")
        Call EntradaCompra2
    End If
    
    If (posicao <= 20) Then
        Call Atualizar("66,66", "5.1.3 Entrada Compra 3")
        Call EntradaCompra3
    End If
    
    If (posicao <= 21) Then
        Call Atualizar("70,37", "5.1.4 Entrada Compra 4")
        Call EntradaCompra4
        Call VerificacaoDados1
    End If

    If (posicao <= 22) Then
        Call Atualizar("74,07", "5.2.1 DAV 1")
        Call DAV1
    End If
    
    If (posicao <= 23) Then
        Call Atualizar("77,77", "5.2.2 DAV 2")
        Call DAV2
        Call VerificacaoDados2
    End If

    If (posicao <= 24) Then
        Call Atualizar("81,48", "5.3 Orçamento")
        Call Orcamento
    End If
    
    If (posicao <= 25) Then
        Call Atualizar("85,18", "5.4 Condicional")
        Call Condicional
    End If
    
    If (posicao <= 26) Then
        Call Atualizar("88,88", "7.1 Lançamentos Avulsos CR")
        Call LancamentoCR
    End If
    
    If (posicao <= 27) Then
        Call Atualizar("92,59", "7.3 Lançamentos Avulsos CP")
        Call LancamentoCP
    End If
    
    If (posicao <= 28) Then
        Call Atualizar("96,29", "7.5 Lançamentos Avulsos MF")
        Call MovimentoFinanceiro
    End If
    
    If (posicao <= 29) Then
        Call Atualizar("98,59", "8 Financeiro")
        Call RecebimentoPagamento
    End If
    
    
    Call LimparDados 'Fim da verificação
    
    Call Atualizar("100", "Finalizado")
    
    'Call AlterarPessoa
    SendKeys ("{NUMLOCK}")
    MsgBox "Processo encerrado"
End Sub


Sub CriarEmpresa()
   'Criando o campo "Empresa"
        Call LeftClick(665, 335)
        SendKeys "{ENTER}"
        SendKeys "{F2}" 'criando nova pessoa (empresa)
        SendKeys "{DOWN}"
        SendKeys "{ENTER}" 'selecionando juridica
        SendKeys "04248801000121"
        SendKeys "{ENTER}" 'informando o cnpj
        SendKeys "{UP}"
        SendKeys "{ENTER}" 'informando se é contribuinte
        SendKeys "002.149.581/0097"
        SendKeys "{ENTER}"
        SendKeys "#empresa1"
        SendKeys "{ENTER 2}"
        SendKeys "{ENTER}" 'abrindo a tela de logradouro
        SendKeys "{F2}"
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys "4"
        SendKeys "{ENTER}"
        SendKeys "Rui Barbosa"
        SendKeys "{ENTER 2}"
        Application.Wait (Now + TimeValue("0:00:03"))
        Call LeftClick(842, 338) 'selecionando o logradouro criado
        SendKeys "{F5}"
        SendKeys "{ENTER 3}"
        SendKeys "{F2}" 'Abrindo a tela de bairro
        Application.Wait (Now + TimeValue("0:00:01"))
        Call LeftClick(665, 335)
        SendKeys "Centro"
        SendKeys "{ENTER 2}"
        Application.Wait (Now + TimeValue("0:00:03"))
        Call LeftClick(842, 338) 'selecionando o bairro criado
        SendKeys "38800000" 'Informando o CEP
        SendKeys "{ENTER}"
        SendKeys "{ENTER}"
        SendKeys "3162104" 'informando o município SG
        Call LeftClick(600, 756)
        Application.Wait (Now + TimeValue("0:00:03"))
        Call LeftClick(842, 338) 'Selecionando a pessoa da empresa
 'Fim do campo "Empresa"
    SendKeys "{DOWN}"
    SendKeys "{ENTER}" 'selecionando o campo Perfil do Informante
    
    'criando o campo "Responsável Legal"
        Call LeftClick(914, 453)
        Call LeftClick(914, 453)
        SendKeys "{F2}" 'criando nova pessoa (Perfil)
        SendKeys "{ENTER}"
        SendKeys "62688825631" 'cpf da pessoa
        SendKeys "{ENTER 3}"
        SendKeys "#pessoaResponsável" 'nome da pessoa
        SendKeys "{ENTER 2}"
        SendKeys "1" 'informando o logradouro
        SendKeys "{ENTER 3}"
        SendKeys "1" 'informando o bairro
        SendKeys "{ENTER}"
        SendKeys "38800000" 'informando o CEP
        SendKeys "{ENTER 2}"
        SendKeys "3162104" 'informando o município SG
        Call LeftClick(600, 756)
        Application.Wait (Now + TimeValue("0:00:03"))
        SendKeys "{DOWN}"
        Call LeftClick(842, 338) 'Selecionando a pessoa responsável
    'fim da criação da pessoa responsável
    
    Call LeftClick(662, 717)
    Call LeftClick(662, 717) 'Salvando a empresa
    Application.Wait (Now + TimeValue("0:00:04"))
    
    Call LeftClick(842, 338) 'selecionando a empresa criado
    Application.Wait (Now + TimeValue("0:00:15"))

End Sub



Sub Parametrizacao()

