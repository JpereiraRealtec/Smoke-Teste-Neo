    Application.Wait (Now + TimeValue("0:00:05"))
    
    Call LeftClick(542, 208) 'acessando a guia itens
    Call LeftClick(824, 438)
    Call LeftClick(1437, 433)
    '---Clicando no botão de acertar variação
        Application.Wait (Now + TimeValue("0:00:02"))
        SendKeys ("50") 'informando a quantidade
        SendKeys ("{ENTER}")
        SendKeys ("A1B1") 'informando a Barras
        SendKeys ("{ENTER 4}") 'salvando a variação
        Application.Wait (Now + TimeValue("0:00:02"))
        SendKeys ("50") 'informando a quantidade
        SendKeys ("{ENTER}")
        SendKeys ("A2B2") 'informando a Barras
        SendKeys ("{ENTER 4}") 'salvando a variação
        Application.Wait (Now + TimeValue("0:00:02"))
        Call LeftClick(598, 759) 'salvando
    '---Fim do acerto de variação
    Application.Wait (Now + TimeValue("0:00:03"))
    
    Call RightClick(672, 403)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))

    '--- verificação dos produtos inseridos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        Dim contador As Integer
        contador = 2
        Do While (contador < 5)
            If (Sheets(2).Range("F" & contador).Value <> "500") Then
                MsgBox "Ocorreu algum erro com o CFOP da Entrada Compra" & vbCrLf & "Valor esperado: 500" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
        
        contador = 2
        Do While (contador < 5)
            If (Sheets(2).Range("G" & contador).Value <> "1102") And (Sheets(2).Range("G" & contador).Value <> "1,102") Then
                MsgBox "Ocorreu algum erro com o CFOP da Entrada Compra" & vbCrLf & "Valor esperado: 1.102" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G" & contador).Value
                End
            End If
            contador = contador + 1
        Loop

        If (Sheets(2).Range("I2").Value <> "10") Or (Sheets(2).Range("I3").Value <> "10") Then
            MsgBox "Ocorreu algum erro com o unitário da Entrada Compra" & vbCrLf & "Valor esperado: 10/10" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I2").Value & "/" & Sheets(2).Range("I3").Value
            End
        End If
    
        If (Sheets(2).Range("I4").Value <> "50") Then
            MsgBox "Ocorreu algum erro com o unitário da Entrada Compra" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I4").Value
            End
        End If

        If (Sheets(2).Range("K2").Value <> "10") Or (Sheets(2).Range("K3").Value <> "10") Then
            MsgBox "Ocorreu algum erro com o Líquido da Entrada Compra" & vbCrLf & "Valor esperado: 10/10" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K2").Value & "/" & Sheets(2).Range("K3").Value
            End
        End If
        
        If (Sheets(2).Range("K4").Value <> "5000") Then
            MsgBox "Ocorreu algum erro com o Líquido da Entrada Compra" & vbCrLf & "Valor esperado: 5000" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K4").Value
            End
        End If

        If (Sheets(2).Range("L2").Value <> "10") Or (Sheets(2).Range("L3").Value <> "10") Then
            MsgBox "Ocorreu algum erro com a Base da Entrada Compra" & vbCrLf & "Valor esperado: 10/10" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L2").Value & "/" & Sheets(2).Range("L3").Value
            End
        End If
        
        If (Sheets(2).Range("L4").Value <> "5000") Then
            MsgBox "Ocorreu algum erro com a Base da Entrada Compra" & vbCrLf & "Valor esperado: 5000" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L4").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("M2").Value) <> "1,2") Or (CStr(Sheets(2).Range("M3").Value) <> "1,2") Then
            MsgBox "Ocorreu algum erro com o Valor ICMS da Entrada Compra" & vbCrLf & "Valor esperado: 1,2/1,2" & vbCrLf & "Valor obtido:" & Sheets(2).Range("M2").Value & "/" & Sheets(2).Range("M3").Value
            End
        End If
        
        If (Sheets(2).Range("M4").Value <> "600") Then
            MsgBox "Ocorreu algum erro com o Valor ICMS da Entrada Compra" & vbCrLf & "Valor esperado: 600" & vbCrLf & "Valor obtido:" & Sheets(2).Range("M4").Value
            End
        End If
        
        Sheets(2).Range("A1:Z12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
    'fim da verificação dos produtos inseridos
    
    Call LeftClick(503, 837)   'clicando no estoque
    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(574, 841)
    Application.Wait (Now + TimeValue("0:00:03"))
    Call LeftClick(570, 840)   'clicando no financeiro
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("S") 'Clicando no botão Sim
    Application.Wait (Now + TimeValue("0:00:03"))
    Call LeftClick(1236, 453)   'clicando no botão Gerar
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(670, 743)   'salvando o financeiro
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys "{ESC}" 'fechando a tela de inclusao da Entrada compra
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys "{ESC}" 'fechando a tela de XML
    Application.Wait (Now + TimeValue("0:00:01"))
End Sub


Sub VerificacaoDados1()
    Call LeftClick(90, 68) 'Buscando a tela de Produto
    Call LeftClick(40, 122)
    Call LeftClick(776, 610)
    Application.Wait (Now + TimeValue("0:00:02"))
    
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos produtos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("D2").Value <> "10") Or (Sheets(2).Range("D3").Value <> "10") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do Produto" & vbCrLf & "Valor esperado: 10/10" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D2").Value & Sheets(2).Range("D3").Value
            End
        End If
        
        If (Sheets(2).Range("D4").Value <> "50") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do Produto" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D4").Value
            End
        End If
        
        If (Sheets(2).Range("F2").Value <> "201") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do Produto" & vbCrLf & "Valor esperado: 201" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If
        
        If (Sheets(2).Range("F3").Value <> "132") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do Produto" & vbCrLf & "Valor esperado: 132" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F3").Value
            End
        End If
        
        If (Sheets(2).Range("F4").Value <> "100") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do Produto" & vbCrLf & "Valor esperado: 100" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F4").Value
            End
        End If
        
        Dim contador As Integer
        contador = 2
        Do While (contador < 5)
            If (Sheets(2).Range("AE" & contador).Value <> "3105.90.90") Then
                MsgBox "Ocorreu algum erro com " & Sheets(2).Range("AE1").Value & " do Produto" & vbCrLf & "Valor esperado: 3105.90.90" & vbCrLf & "Valor obtido:" & Sheets(2).Range("AE" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
        
        If (CStr(Sheets(2).Range("Z2").Value) <> "259383084577114") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("Z1").Value & " do Produto" & vbCrLf & "Valor esperado: 2,59383084577114" & vbCrLf & "Valor obtido:" & Sheets(2).Range("Z2").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("Z3").Value) <> "153684848484848") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("Z1").Value & " do Produto" & vbCrLf & "Valor esperado: 15,3684848484848" & vbCrLf & "Valor obtido:" & Sheets(2).Range("Z3").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("Z4").Value) <> "50") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("Z1").Value & " do Produto" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("Z4").Value
            End
        End If
    '---- fim da verificação de dados dos produtos
    
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    SendKeys "{ESC}" 'saindo da tela de produtos
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call LeftClick(325, 65) 'Buscando a tela de Contas a Pagar
    Call LeftClick(95, 120)
    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(788, 673)
    Application.Wait (Now + TimeValue("0:00:02"))
    
    
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos Contas a Pagar
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("E2").Value <> "6431/1") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 6431/1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E2").Value
            End
        End If
        
        If (Sheets(2).Range("E3").Value <> "59769/1") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 59769/1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E3").Value
            End
        End If
        
        If (Sheets(2).Range("E4").Value <> "59769/2") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 59769/2" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E4").Value
            End
        End If
        
        If (Sheets(2).Range("E5").Value <> "31075/1") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 31075/1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E5").Value
            End
        End If
        
        If (Sheets(2).Range("E6").Value <> "62268/1") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 62268/1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E6").Value
            End
        End If

        If (Sheets(2).Range("G2").Value <> "603") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 603" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G2").Value
            End
        End If
        
        If (Sheets(2).Range("G3").Value <> "950") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 950" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G3").Value
            End
        End If
        
        If (Sheets(2).Range("G4").Value <> "950") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 950" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G4").Value
            End
        End If
        
        If (Sheets(2).Range("G5").Value <> "27") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 27" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G5").Value
            End
        End If
        
        If (Sheets(2).Range("G6").Value <> "5020") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 5020" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G6").Value
            End
        End If
        
        If (Sheets(2).Range("P2").Value <> "158") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("P1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 158" & vbCrLf & "Valor obtido:" & Sheets(2).Range("P2").Value
            End
        End If
        
        contador = 3
        Do While (contador < 7)
            If (Sheets(2).Range("P" & contador).Value <> "162") Then
                MsgBox "Ocorreu algum erro com " & Sheets(2).Range("P1").Value & " do Contas a Pagar" & vbCrLf & "Valor esperado: 162" & vbCrLf & "Valor obtido:" & Sheets(2).Range("P" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
        
        contador = 2
        Do While (contador < 7)
            If (Sheets(2).Range("K" & contador).Value <> "Aberto    ") Then
                MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & "do Contas a Pagar" & vbCrLf & "Valor esperado: Aberto" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
    '---- fim da verificação de dados do Contas a Pagar
    
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    SendKeys "{ESC}" 'saindo da tela de Contas a Pagar
    Application.Wait (Now + TimeValue("0:00:01"))
End Sub



Sub DAV1()
    Call LeftClick(206, 66) 'Buscando a tela de DAV
    Call LeftClick(37, 116)
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(705, 717)
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{F2}") 'abrindo a tela de inclusão do dav
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys ("{ENTER 2}")
    SendKeys ("5102") 'informando o CFOP
    SendKeys ("{ENTER}")
    SendKeys ("1") 'informando o cliente
    SendKeys ("{ENTER 2}")
    SendKeys ("1") 'informando o funcionário
    SendKeys ("{ENTER}")
    SendKeys ("4") 'informando a forma de pagamento
    SendKeys ("{ENTER 5}")
    Application.Wait (Now + TimeValue("0:00:02"))
    
    '---Incluindo os itens
        SendKeys ("{F1}") 'alterando o tipo do item (de serviço para produto)
        SendKeys ("100")
        SendKeys ("{ENTER}")
        SendKeys ("1")
        SendKeys ("{ENTER 5}") 'incluindo o primeiro item
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys ("50")
        SendKeys ("{ENTER}")
        SendKeys ("2")
        SendKeys ("{ENTER 5}") 'incluindo o segundo item
        Application.Wait (Now + TimeValue("0:00:01"))
    '--- Fim da inclusão dos itens
    
    SendKeys ("{F5}") 'abrindo a tela de desconto
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("30")
    SendKeys ("{ENTER 3}") 'salvando o desconto
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call RightClick(665, 423)
    SendKeys ("T") 'copiando a grade de produto
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos produtos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("D2").Value <> "100") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do DAV" & vbCrLf & "Valor esperado: 100" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D2").Value
            End
        End If
        
        If (Sheets(2).Range("D3").Value <> "50") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do DAV" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D3").Value
            End
        End If
        
        If (Sheets(2).Range("I2").Value <> "3") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " do DAV" & vbCrLf & "Valor esperado: 3" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I2").Value
            End
        End If
        
        If (Sheets(2).Range("I3").Value <> "20") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " do DAV" & vbCrLf & "Valor esperado: 20" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I3").Value
            End
        End If
        
        If (Sheets(2).Range("K2").Value <> "6,92") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " do DAV" & vbCrLf & "Valor esperado: 6,92" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K2").Value
            End
        End If
        
        If (Sheets(2).Range("K3").Value <> "23,08") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " do DAV" & vbCrLf & "Valor esperado: 23,08" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K3").Value
            End
        End If
    
        If (Sheets(2).Range("L2").Value <> "293,08") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " do DAV" & vbCrLf & "Valor esperado: 293,08" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L2").Value
            End
        End If
        
        If (Sheets(2).Range("L3").Value <> "976,92") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " do DAV" & vbCrLf & "Valor esperado: 976,92" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L3").Value
            End
        End If
    
    
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    
    Call LeftClick(496, 850) 'clicando no botão estoque
    Application.Wait (Now + TimeValue("0:00:01"))
    '--- Tela de parametros de contabilização
        SendKeys ("S") 'confirmando a mensagem de CMV
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys ("{ENTER 2}")
        Application.Wait (Now + TimeValue("0:00:03"))
        Call LeftClick(604, 314)
        SendKeys ("{ENTER 8}")
        SendKeys ("76") 'informando o campo CMV
        SendKeys ("{ENTER 2}")
        Application.Wait (Now + TimeValue("0:00:02"))
        SendKeys ("{F2}") 'criando um novo histórico
        Application.Wait (Now + TimeValue("0:00:02"))
        SendKeys ("Histórico de MVA")
        SendKeys ("{ENTER 2}")
        Application.Wait (Now + TimeValue("0:00:02"))
        SendKeys ("{ENTER}")
        Application.Wait (Now + TimeValue("0:00:02"))
        SendKeys ("{ENTER 2}") 'salvando os parametros
    '--- Fim da tela de parametros de contabilização
    
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys ("{ENTER}") 'abrindo a tela de Financeiro
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys ("{ENTER 2}") 'gerando as parcelas
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{ENTER}") 'salvando o financeiro
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{RIGHT}")
    SendKeys ("{ENTER}") 'fechando a tela de financeiro (clicando no botão Sair)
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{ESC}") 'saindo da tela de inclusão do DAV
    
    Call LeftClick(611, 346) 'clicando na tela de filtro
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(710, 606) 'clicando no campo Número Inicial
    SendKeys ("1")
    SendKeys ("{ENTER 5}")
    Application.Wait (Now + TimeValue("0:00:02"))
    
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos produtos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
    
        If (Sheets(2).Range("E2").Value <> "1270") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do DAV" & vbCrLf & "Valor esperado: 1270" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E2").Value
            End
        End If
    
        If (Sheets(2).Range("F2").Value <> "Confirmado") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do DAV" & vbCrLf & "Valor esperado: Confirmado" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If
        
        If (Sheets(2).Range("G2").Value <> "S") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do DAV" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G2").Value
            End
        End If
        
        If (Sheets(2).Range("H2").Value <> "S") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("H1").Value & " do DAV" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("H2").Value
            End
        End If

    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    SendKeys ("{ESC}") 'saindo da tela de consulta do DAV
    Application.Wait (Now + TimeValue("0:00:01"))
End Sub



