Sub CadastrarUsuário()
    Call LeftClick(90, 68) 'Buscando a tela de Usuário
    Call LeftClick(547, 128)
    Call LeftClick(568, 304)
    
    SendKeys "{F2}" 'Criando um novo usuário
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "#usuário1"
    SendKeys "{ENTER}"
    SendKeys "U1"
    SendKeys "{ENTER}"
    SendKeys "1"
    SendKeys "{ENTER}"
    SendKeys "1"
    Call LeftClick(762, 602) 'aqui salva as informações do usuário
    
    Application.Wait (Now + TimeValue("0:00:03"))
    Call LeftClick(963, 340) 'selecionando o privilégio do usuário
    SendKeys "2"
    SendKeys "{ENTER 2}" 'Salvando o privilégio
    
    
    Application.Wait (Now + TimeValue("0:00:03"))
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "{ESC}" 'fechando a tela
    
    '--- verificação de dados
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("A2").Value <> "#usuário1") Then
            MsgBox "Ocorreu algum erro com a descrição do usuário" & vbCrLf & "Valor esperado: #usuário1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A2").Value
            End
        End If
        If (Sheets(2).Range("B2").Value <> "U1") Then
            MsgBox "Ocorreu algum erro com a descrição do usuário" & vbCrLf & "Valor esperado: U1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B2").Value
            End
        End If
        If (Sheets(2).Range("D2").Value <> "S") Then
            MsgBox "Ocorreu algum erro em relação ao Ativo/Inativo do usuário" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D2").Value
            End
        End If
        If (Sheets(2).Range("E2").Value <> "Gerência") Then
            MsgBox "Ocorreu algum erro com o privilégio do usuário" & vbCrLf & "Valor esperado: Gerência" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E2").Value
            End
        End If
    '---- fim da verificação de dados
    
    '---- limpando os dados
        Sheets(2).Range("A1:H12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
End Sub


Sub CadastrarFuncionario()
    Call LeftClick(90, 68) 'Buscando a tela de Funcionário
    Call LeftClick(485, 128)
    
    SendKeys "{F2}" 'Criando um novo funcionário
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "2" 'campo Pessoa
    SendKeys "{ENTER}"
    SendKeys "1" 'campo usuario
    SendKeys "{ENTER 3}"
    SendKeys "{DOWN}"
    SendKeys "{ENTER}"
    SendKeys "{DOWN}"
    SendKeys "{ENTER}"
    SendKeys "{DOWN}"
    SendKeys "{ENTER}"
    SendKeys "{DOWN}"
    SendKeys "{ENTER}"
    SendKeys "{DOWN}"
    SendKeys "{ENTER}"
    SendKeys "50" 'campo financeiro
    SendKeys "{ENTER}"
    SendKeys "75" 'campo comercial
    SendKeys "{ENTER 2}" 'aqui salva as informações do funcionário
    
    Application.Wait (Now + TimeValue("0:00:03"))
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys "{ESC}" 'fechando a tela
    
    '--- verificação de dados
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("A2").Value <> "#pessoaresponsável") Then
            MsgBox "Ocorreu algum erro com a descrição do funcionário" & vbCrLf & "Valor esperado: #pessoaresponsável" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A2").Value
            End
        End If
        If (Sheets(2).Range("B2").Value <> "1") Then
            MsgBox "Ocorreu algum erro com o código interno do funcionário" & vbCrLf & "Valor esperado: 1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B2").Value
            End
        End If
        If (Sheets(2).Range("C2").Value <> "S") Then
            MsgBox "Ocorreu algum erro em relação ao Ativo/Inativo do funcionário" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("C2").Value
            End
        End If

    '---- fim da verificação de dados
    
    '---- limpando os dados
        Sheets(2).Range("A1:H12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
    
End Sub

Sub CadastrarMunicipio()
    Call LeftClick(90, 68) 'Buscando a tela de Municipio
    Call LeftClick(543, 130)
    Call LeftClick(624, 240)
    SendKeys "M"
    SendKeys "{F2}" 'abrindo a tela de inclusão de Municipio
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "#ex"
    SendKeys "{ENTER}"
    SendKeys "1198"
    SendKeys "{ENTER 2}" 'salvando o cadastro
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys "{ESC}" 'fechando a tela
    
End Sub


Sub CadastrarContaMovimento()
    Call LeftClick(322, 68) 'Buscando a tela de Conta Movimento
    Call LeftClick(761, 125)
    Call LeftClick(802, 195)
    SendKeys "{F2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "5"
    SendKeys "{ENTER 4}" 'salvando a primeira conta movimento
    
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys "{F2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "7"
    SendKeys "{ENTER}"
    SendKeys "{DOWN}"
    SendKeys "{ENTER 3}" 'salvando a segunda conta movimento
    Application.Wait (Now + TimeValue("0:00:03"))
    
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys "{ESC}" 'fechando a tela
    
    '--- verificação de dados
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("A2").Value <> "Banco do Brasil S/A.") Then
            MsgBox "Ocorreu algum erro com a descrição da conta movimento" & vbCrLf & "Valor esperado: Banco do Brasil S/A." & vbCrLf & "Valor obtido:" & Sheets(2).Range("A2").Value
            End
        End If
        If (Sheets(2).Range("C2").Value <> "C. Corrente") Then
            MsgBox "Ocorreu algum erro com o tipo da conta movimento" & vbCrLf & "Valor esperado: C. Corrente" & vbCrLf & "Valor obtido:" & Sheets(2).Range("C2").Value
            End
        End If
        If (Sheets(2).Range("E2").Value <> "S") Then
            MsgBox "Ocorreu algum erro em relação ao Ativo/Inativo" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E2").Value
            End
        End If
        'verificação da conta 2
        If (Sheets(2).Range("A3").Value <> "Caixa Geral") Then
            MsgBox "Ocorreu algum erro com a descrição da conta movimento" & vbCrLf & "Valor esperado: Caixa Geral" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A3").Value
            End
        End If
        If (Sheets(2).Range("C3").Value <> "Caixa      ") Then
            MsgBox "Ocorreu algum erro com o tipo da conta movimento" & vbCrLf & "Valor esperado: Caixa" & vbCrLf & "Valor obtido:" & Sheets(2).Range("C3").Value
            End
        End If
        If (Sheets(2).Range("E3").Value <> "S") Then
            MsgBox "Ocorreu algum erro em relação ao Ativo/Inativo da Conta Movimento" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E3").Value
            End
        End If

    '---- fim da verificação de dados
    
    '---- limpando os dados
        Sheets(2).Range("A1:W12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
End Sub


Sub CadastrarClassificacao()
    Call LeftClick(90, 68) 'Buscando a tela de classificação
    Call LeftClick(543, 130)
    Call LeftClick(587, 195)
    SendKeys "{F2}" 'cadastrando a classificação
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "Geral"
    SendKeys "{ENTER 4}"
    SendKeys "{UP}"
    SendKeys "{ENTER}"
    SendKeys "{UP}"
    Call LeftClick(776, 446)
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(782, 611)
    Call LeftClick(782, 611)
    Call LeftClick(793, 613)
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(709, 660) 'salvando as informações
    
    Application.Wait (Now + TimeValue("0:00:03"))
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys "{ESC}" 'fechando a tela
    
    '--- verificação de dados
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("A2").Value <> "Geral") Then
            MsgBox "Ocorreu algum erro com a descrição da classificação" & vbCrLf & "Valor esperado: Geral" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A2").Value
            End
        End If
        If (Sheets(2).Range("B2").Value <> "1") Then
            MsgBox "Ocorreu algum erro com o código interno da classificação" & vbCrLf & "Valor esperado: 1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B2").Value
            End
        End If
        
        Dim cont As Integer
        cont = 0
        
        Do While (cont < 16) 'verifica os bloqueios
            If (Sheets(2).Range(Chr(67 + cont) & "2").Value <> "Não     ") Then
                MsgBox "Ocorreu algum erro em relação ao " & Sheets(2).Range(Chr(67 + cont) & "1").Value & vbCrLf & "Valor esperado: Não" & vbCrLf & "Valor obtido:" & Sheets(2).Range(Chr(67 + cont) & "2").Value
                End
            End If
            
            cont = cont + 1
            If (cont = 12) Then
                cont = 14
            End If
        Loop 'fim da verificação dos bloqueios
        
        If (Sheets(2).Range("S2").Value <> "0") Then
            MsgBox "Ocorreu algum erro com o Bloqueio Venda a Prazo" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("S2").Value
            End
        End If
        
        If (Sheets(2).Range("T2").Value <> "0") Then
            MsgBox "Ocorreu algum erro com o Cobrar" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("T2").Value
            End
        End If
        
        If (Sheets(2).Range("V2").Value <> "S") Then
            MsgBox "Ocorreu algum erro com o Ativo/Inativo da Classificação" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("V2").Value
            End
        End If

    '---- fim da verificação de dados
    
    '---- limpando os dados
        Sheets(2).Range("A1:Z12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
End Sub


Sub CadastrarMarca()
    Call LeftClick(90, 68) 'Buscando a tela de Marca
    Call LeftClick(97, 123)
    Call LeftClick(116, 240)
    SendKeys "{F2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "Teste Marca"
    Call LeftClick(829, 615) 'salvando a marca
    Call LeftClick(825, 581)
    
    Application.Wait (Now + TimeValue("0:00:03"))
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys "{ESC}" 'fechando a tela
    
    

    '--- verificação de dados
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("A2").Value <> "Teste Marca") Then
            MsgBox "Ocorreu algum erro com a descrição da Marca" & vbCrLf & "Valor esperado: Teste Marca" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A2").Value
            End
        End If
        If (Sheets(2).Range("B2").Value <> "1") Then
            MsgBox "Ocorreu algum erro com o código interno da Marca" & vbCrLf & "Valor esperado: 1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B2").Value
            End
        End If
        If (Sheets(2).Range("C2").Value <> "S") Then
            MsgBox "Ocorreu algum erro com o Ativo/Inativo da marca" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("C2").Value
            End
        End If

    '---- fim da verificação de dados
    
    '---- limpando os dados
        Sheets(2).Range("A1:Z12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
End Sub


