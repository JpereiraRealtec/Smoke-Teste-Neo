Sub MovimentoFinanceiro()
    Call LeftClick(326, 66)
    Call LeftClick(156, 116) 'Acessando a tela de movimento financeiro
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "1"
    SendKeys "{ENTER 4}" 'confirmando o filtro
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '------ Incluindo o primeiro avulso Movimento Financeiro
    SendKeys "{F2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "{ENTER}"
    SendKeys "137" 'Conta Contabil
    SendKeys "{ENTER 3}"
    SendKeys "10000" 'Valor
    SendKeys "{ENTER}"
    SendKeys "Lam1" 'N Documento
    SendKeys "{ENTER 4}"
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '------ Incluindo o primeiro avulso Movimento Financeiro
    SendKeys "{F2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "{DOWN}" 'Alterando o Tipo de Operação
    SendKeys "{ENTER}"
    SendKeys "26" 'Conta Contabil
    SendKeys "{ENTER 3}"
    SendKeys "50" 'Valor
    SendKeys "{ENTER}"
    SendKeys "Lam2" 'N Documento
    SendKeys "{ENTER 4}"
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call CopiarDados(747, 432)
    
        Call VerificarDados("1974,8", "E2", "Movimento Financeiro Pagamento do DAV")
        Call VerificarDados("10000", "E3", "Movimento Financeiro Avulso") 'Valor
        Call VerificarDados("50", "E4", "Movimento Financeiro Avulso")
    
        Call VerificarDados("Crédito", "D2", "Movimento Financeiro Pagamento do DAV")
        Call VerificarDados("Crédito", "D3", "Movimento Financeiro Avulso") 'Natureza
        Call VerificarDados("Débito ", "D4", "Movimento Financeiro Avulso")
        
    Call LimparDados 'Fim da verificação
    
    SendKeys "{ESC}"
End Sub


Sub RecebimentoPagamento()
    Call LeftClick(326, 66)
    Call LeftClick(156, 116) 'Acessando a tela de movimento financeiro
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "2"
    SendKeys "{ENTER 4}" 'confirmando o filtro
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys "{F7}"
    '------ Tela de Recebimento
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys "{ENTER 6}"
        SendKeys "1" 'Informando o Cliente
        SendKeys "{ENTER 2}"
        Application.Wait (Now + TimeValue("0:00:01"))
        Call LeftClick(803, 477)
        Call LeftClick(803, 477)
        Call LeftClick(803, 477)
        SendKeys "{F7}" 'Selecionando todos os registros
        Application.Wait (Now + TimeValue("0:00:01"))
        Call LeftClick(504, 833) 'Clicando no botão Salvar
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys "{ESC}" 'Saindo da tela
        Application.Wait (Now + TimeValue("0:00:01"))
        
    SendKeys "{F8}"
    '------ Tela de Pagamento
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys "{ENTER 6}"
        Application.Wait (Now + TimeValue("0:00:01"))
        Call LeftClick(803, 477)
        Call LeftClick(803, 477)
        Call LeftClick(803, 477)
        SendKeys "{F7}" 'Selecionando todos os registros
        Call LeftClick(505, 842)
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys "{ESC}" 'Saindo da tela
        Application.Wait (Now + TimeValue("0:00:01"))
        
    Call CopiarDados(707, 442)
    
        Call VerificarDados("3770", "E2", "Movimento Financeiro na hora de Pagamento/Recebimento") 'Valor
        Call VerificarDados("1703", "E3", "Movimento Financeiro na hora de Pagamento/Recebimento")
        Call VerificarDados("6947", "E4", "Movimento Financeiro na hora de Pagamento/Recebimento")
        
        Call VerificarDados("Crédito", "D2", "Movimento Financeiro na hora de Pagamento/Recebimento") 'Natureza
        Call VerificarDados("Débito ", "D3", "Movimento Financeiro na hora de Pagamento/Recebimento")
        Call VerificarDados("Débito ", "D4", "Movimento Financeiro na hora de Pagamento/Recebimento")
        
    Call LimparDados 'Fim da verificação
    
    SendKeys "{ESC}" 'Saindo da tela
End Sub
