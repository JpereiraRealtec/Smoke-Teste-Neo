    SendKeys ("1") 'informando o funcionário
    SendKeys ("{ENTER}")
    SendKeys ("1") 'informando a forma de pagamento
    SendKeys ("{ENTER 5}")
    Application.Wait (Now + TimeValue("0:00:02"))
    
    '---Incluindo os itens
        SendKeys ("{F1}") 'alterando o tipo do item (de serviço para produto)
        SendKeys ("10")
        SendKeys ("{ENTER}")
        SendKeys ("1")
        SendKeys ("{ENTER 5}") 'incluindo o primeiro item
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys ("15")
        SendKeys ("{ENTER}")
        SendKeys ("2")
        SendKeys ("{ENTER 5}") 'incluindo o segundo item
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys ("50")
        SendKeys ("{ENTER}")
        SendKeys ("3")
        SendKeys ("{ENTER 5}") 'abrindo a tela de variação
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys ("25")
        SendKeys ("{ENTER}")
        SendKeys ("25")
        SendKeys ("{ENTER 3}")
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys ("{ENTER}") 'incluindo o terceiro item
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys ("{F1}") 'alterando o tipo do item (de produto para serviço)
        SendKeys ("1")
        SendKeys ("{ENTER}")
        SendKeys ("1")
        SendKeys ("{ENTER 5}") 'incluindo o quarto item (serviço)
        Application.Wait (Now + TimeValue("0:00:01"))
    '--- Fim da inclusão dos itens
    
    SendKeys ("{F5}") 'abrindo a tela de desconto
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("5,2")
    SendKeys ("{ENTER 3}") 'salvando o desconto
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call RightClick(665, 423)
    SendKeys ("T") 'copiando a grade de produto
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos produtos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("D2").Value <> "1") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do DAV" & vbCrLf & "Valor esperado: 1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D2").Value
            End
        End If
        
        If (Sheets(2).Range("D3").Value <> "15") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do DAV" & vbCrLf & "Valor esperado: 15" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D3").Value
            End
        End If
        
        If (Sheets(2).Range("D4").Value <> "10") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do DAV" & vbCrLf & "Valor esperado: 10" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D4").Value
            End
        End If
        
        If (Sheets(2).Range("D5").Value <> "50") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do DAV" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D5").Value
            End
        End If
        
        If (Sheets(2).Range("I2").Value <> "150") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " do DAV" & vbCrLf & "Valor esperado: 150" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I2").Value
            End
        End If
        
        If (Sheets(2).Range("I3").Value <> "20") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " do DAV" & vbCrLf & "Valor esperado: 20" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I3").Value
            End
        End If
        
        If (Sheets(2).Range("I4").Value <> "3") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " do DAV" & vbCrLf & "Valor esperado: 3" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I4").Value
            End
        End If
        
        If (Sheets(2).Range("I5").Value <> "30") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " do DAV" & vbCrLf & "Valor esperado: 30" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I5").Value
            End
        End If
        
        If (Sheets(2).Range("K2").Value <> "0") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " do DAV" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K2").Value
            End
        End If
        
        If (Sheets(2).Range("K3").Value <> "0,85") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " do DAV" & vbCrLf & "Valor esperado: 0,85" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K3").Value
            End
        End If
        
        If (Sheets(2).Range("K4").Value <> "0,08") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " do DAV" & vbCrLf & "Valor esperado: 0,08" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K4").Value
            End
        End If
        
        If (Sheets(2).Range("K5").Value <> "4,27") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " do DAV" & vbCrLf & "Valor esperado: 4,27" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K5").Value
            End
        End If
        
        If (Sheets(2).Range("L2").Value <> "150") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " do DAV" & vbCrLf & "Valor esperado: 150" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L2").Value
            End
        End If
        
        If (Sheets(2).Range("L3").Value <> "299,15") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " do DAV" & vbCrLf & "Valor esperado: 299,15" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L3").Value
            End
        End If
        
        If (Sheets(2).Range("L4").Value <> "29,92") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " do DAV" & vbCrLf & "Valor esperado: 29,92" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L4").Value
            End
        End If
        
        If (Sheets(2).Range("L5").Value <> "1495,73") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " do DAV" & vbCrLf & "Valor esperado: 1495,73" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L5").Value
            End
        End If
    
    
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""

    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(496, 850) 'clicando no botão estoque
    Application.Wait (Now + TimeValue("0:00:02"))
    SendKeys ("{ENTER}") 'abrindo a tela de Financeiro
    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(500, 836)
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("1")
    SendKeys ("{ENTER}")
    Call LeftClick(500, 836) 'salvando o financeiro
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{RIGHT 2}")
    SendKeys ("{ENTER}") 'fechando a tela de financeiro (clicando no botão Sair)
    Application.Wait (Now + TimeValue("0:00:01"))

    SendKeys ("{ESC}") 'saindo da tela de inclusão do DAV

    Call LeftClick(611, 346) 'clicando na tela de filtro
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(710, 606) 'clicando no campo Número Inicial
    SendKeys ("2")
    SendKeys ("{ENTER 5}")
    Application.Wait (Now + TimeValue("0:00:02"))

    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))

    '--- verificação de dados dos produtos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("E2").Value <> "1974,8") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do DAV" & vbCrLf & "Valor esperado: 1974,8" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E2").Value
            End
        End If

        If (Sheets(2).Range("F2").Value <> "Confirmado") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do DAV" & vbCrLf & "Valor esperado: Confirmado" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If

        If (Sheets(2).Range("G2").Value <> "S") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do DAV" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G2").Value
            End
        End If

        If (Sheets(2).Range("H2").Value <> "S") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("H1").Value & " do DAV" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("H2").Value
            End
        End If

    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    SendKeys ("{ESC}") 'saindo da tela de consulta do DAV
    Application.Wait (Now + TimeValue("0:00:01"))
End Sub




Sub VerificacaoDados2()
    Call LeftClick(90, 68) 'Buscando a tela de Produto
    Call LeftClick(40, 122)
    Call LeftClick(776, 610)
    Application.Wait (Now + TimeValue("0:00:02"))
    
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos produtos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("D2").Value <> "10") Or (Sheets(2).Range("D3").Value <> "10") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do Produto" & vbCrLf & "Valor esperado: 10/10" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D2").Value & Sheets(2).Range("D3").Value
            End
        End If
        
        If (Sheets(2).Range("D4").Value <> "50") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do Produto" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D4").Value
            End
        End If
        
        If (Sheets(2).Range("F2").Value <> "91") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do Produto" & vbCrLf & "Valor esperado: 91" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If
        
        If (Sheets(2).Range("F3").Value <> "67") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do Produto" & vbCrLf & "Valor esperado: 67" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F3").Value
            End
        End If
        
        If (Sheets(2).Range("F4").Value <> "50") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do Produto" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F4").Value
            End
        End If
        
        Dim contador As Integer
        contador = 2
        Do While (contador < 5)
            If (Sheets(2).Range("AE" & contador).Value <> "3105.90.90") Then
                MsgBox "Ocorreu algum erro com " & Sheets(2).Range("AE1").Value & " do Produto" & vbCrLf & "Valor esperado: 3105.90.90" & vbCrLf & "Valor obtido:" & Sheets(2).Range("AE" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
        
        If (CStr(Sheets(2).Range("Z2").Value) <> "259383084577114") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("Z1").Value & " do Produto" & vbCrLf & "Valor esperado: 259383084577114" & vbCrLf & "Valor obtido:" & Sheets(2).Range("Z2").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("Z3").Value) <> "153684848484848") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("Z1").Value & " do Produto" & vbCrLf & "Valor esperado: 153684848484848" & vbCrLf & "Valor obtido:" & Sheets(2).Range("Z3").Value
            End
        End If
        
        If (CStr(Sheets(2).Range("Z4").Value) <> "50") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("Z1").Value & " do Produto" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("Z4").Value
            End
        End If
    '---- fim da verificação de dados dos produtos
    
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    SendKeys "{ESC}" 'saindo da tela de produtos
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call LeftClick(325, 65) 'Buscando a tela de Contas a Receber
    Call LeftClick(32, 121)
    Application.Wait (Now + TimeValue("0:00:02"))
    Call LeftClick(1167, 612)
    Call LeftClick(728, 673)
    Application.Wait (Now + TimeValue("0:00:02"))
    
    
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos Contas a Receber
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Left(Sheets(2).Range("E2").Value, 5) <> "01/01") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & "do Contas a Receber" & vbCrLf & "Valor esperado: 01/01/2020    (01/01)" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E2").Value
            End
        End If
        
        If (Left(Sheets(2).Range("E3").Value, 5) <> "02/01") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & "do Contas a Receber" & vbCrLf & "Valor esperado: 02/01/2020     (01/02)" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E3").Value
            End
        End If
        
        If (Left(Sheets(2).Range("E4").Value, 5) <> "03/01") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: 03/01/2020    (01/03)" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E4").Value
            End
        End If
        
        If (Left(Sheets(2).Range("E5").Value, 5) <> "01/02") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("E1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: 01/02/2020    (02/01)" & vbCrLf & "Valor obtido:" & Sheets(2).Range("E5").Value
            End
        End If

        If (Sheets(2).Range("G2").Value <> "423,34") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: 423,34" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G2").Value
            End
        End If
        
        If (Sheets(2).Range("G3").Value <> "423,33") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: 423,33" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G3").Value
            End
        End If
        
        If (Sheets(2).Range("G4").Value <> "423,33") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: 423,33" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G4").Value
            End
        End If
        
        If (Sheets(2).Range("G5").Value <> "0") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G5").Value
            End
        End If
        
        If (Sheets(2).Range("I5").Value <> "1974,8") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("I1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: 1974,8" & vbCrLf & "Valor obtido:" & Sheets(2).Range("I5").Value
            End
        End If
        
        contador = 2
        Do While (contador < 6)
            If (Sheets(2).Range("S" & contador).Value <> "157") Then
                MsgBox "Ocorreu algum erro com " & Sheets(2).Range("S1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: 157" & vbCrLf & "Valor obtido:" & Sheets(2).Range("S" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
        
        contador = 2
        Do While (contador < 5)
            If (Sheets(2).Range("K" & contador).Value <> "Aberto    ") Then
                MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: Aberto" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K" & contador).Value
                End
            End If
            contador = contador + 1
        Loop
        
        If (Sheets(2).Range("K5").Value <> "Pago      ") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("K1").Value & " do Contas a Receber" & vbCrLf & "Valor esperado: Pago" & vbCrLf & "Valor obtido:" & Sheets(2).Range("K5").Value
            End
        End If
    '---- fim da verificação de dados do Contas a Receber
    
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    SendKeys "{ESC}" 'saindo da tela de Contas a Pagar
    Application.Wait (Now + TimeValue("0:00:01"))
End Sub


Sub Orcamento()
    Call LeftClick(208, 67) 'Buscando a tela de Orçamento
    Call LeftClick(543, 121)
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(1116, 588)
    Call LeftClick(785, 648) 'confirmando o filtro
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "{F2}" 'abrindo a tela de orçamento
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "{ENTER 4}"
    SendKeys "1" 'informando o campo "Funcionario"
    SendKeys "{ENTER}"
    SendKeys "{UP 2}" 'informando a modalidade de frete
    SendKeys "{ENTER}"
    SendKeys "2"
    SendKeys "{ENTER 5}"
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'Incluindo o produto 1
        SendKeys "4" 'informando a quantidade
        SendKeys "{ENTER}"
        SendKeys "{UP}" 'selecionando o Tipo Item = Produto
        SendKeys "{ENTER}"
        SendKeys "1" 'informando o produto
        SendKeys "{ENTER}"
        SendKeys "7" 'informando o valor unitário
        SendKeys "{ENTER}"
        SendKeys "3" 'informando o desconto
        SendKeys "{ENTER 2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    'incluindo o serviço
        SendKeys "1" 'informando a quantidade
        SendKeys "{ENTER 2}"
        SendKeys "{DOWN}" 'informando o Tipo Item = Serviço
        SendKeys "{ENTER}"
        SendKeys "1" 'informando o serviço
        SendKeys "{ENTER}"
        SendKeys "110" 'informando o unitário
        SendKeys "{ENTER}"
        SendKeys "10" 'informando o desconto
        SendKeys "{ENTER 2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    'incluindo o produto não cadastrado
        SendKeys "5" 'informando a quantidade
        SendKeys "{ENTER}"
        SendKeys "{DOWN}" 'informando o Tipo Item = Não cadastrado
        SendKeys "{ENTER}"
        SendKeys "#prod nao cadastrado" 'informando a descrição do produto nao cadastrado
        SendKeys "{ENTER}"
        SendKeys "10" 'informando o unitário
        SendKeys "{ENTER 3}"
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call RightClick(737, 451)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos itens inseridos
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("B2").Value <> "4") Then 'Quantidade
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("B1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 4" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B2").Value
            End
        End If
        
        If (Sheets(2).Range("B3").Value <> "1") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("B1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B3").Value
            End
        End If
        
        If (Sheets(2).Range("B4").Value <> "5") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("B1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 5" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B4").Value
            End
        End If
        
        If (Sheets(2).Range("F2").Value <> "#produto1") Then 'Desrição do item
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & "do Orçamento" & vbCrLf & "Valor esperado: #produto1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If
        
        If (Sheets(2).Range("F3").Value <> "#serviço1") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & "do Orçamento" & vbCrLf & "Valor esperado: #serviço1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F3").Value
            End
        End If
        
        If (Sheets(2).Range("F4").Value <> "#prod Nao Cadastrado") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & "do Orçamento" & vbCrLf & "Valor esperado: #prod Nao Cadastrado" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F4").Value
            End
        End If
        
        If (Sheets(2).Range("G2").Value <> "7") Then 'Unitário
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 7" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G2").Value
            End
        End If
        
        If (Sheets(2).Range("G3").Value <> "110") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 110" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G3").Value
            End
        End If
        
        If (Sheets(2).Range("G4").Value <> "10") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 10" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G4").Value
            End
        End If
        
        If (Sheets(2).Range("H2").Value <> "3") Then 'Desconto
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("H1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 3" & vbCrLf & "Valor obtido:" & Sheets(2).Range("H2").Value
            End
        End If
        
        If (Sheets(2).Range("H3").Value <> "10") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("H1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 10" & vbCrLf & "Valor obtido:" & Sheets(2).Range("H3").Value
            End
        End If
        
        If (Sheets(2).Range("H4").Value <> "0") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("H1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("H4").Value
            End
        End If
        
        If (Sheets(2).Range("J2").Value <> "25") Then 'Total líquido
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("J1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 25" & vbCrLf & "Valor obtido:" & Sheets(2).Range("J2").Value
            End
        End If
        
        If (Sheets(2).Range("J3").Value <> "100") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("J1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 100" & vbCrLf & "Valor obtido:" & Sheets(2).Range("J3").Value
            End
        End If
        
        If (Sheets(2).Range("J4").Value <> "50") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("J1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 50" & vbCrLf & "Valor obtido:" & Sheets(2).Range("J4").Value
            End
        End If
        
    '---- fim da verificação de dados dos itens do orçamento
    
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    
    Call LeftClick(565, 209) 'acessando a guia Forma de Pagamento
    'Primeira forma de pagamento
        SendKeys "5" 'Campo Forma de pagamento
        SendKeys "{ENTER 3}"
        SendKeys " " 'Check box Forma Combinada
        SendKeys "{ENTER 2}"
   'Segunda forma de pagamento
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys "4" 'Campo Forma de pagamento
        SendKeys "{ENTER 4}"
   
    Application.Wait (Now + TimeValue("0:00:01"))
    Call RightClick(668, 318)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
   
    '--- verificação de dados das Formas de Pagamento
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("F2").Value <> "87,5") Then 'Valor Parcela
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do Orçamento" & vbCrLf & "Valor esperado: 87,5" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F2").Value
            End
        End If
        
        If (Sheets(2).Range("F3").Value <> "58,33") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("F1").Value & " do Orçamento" & vbCrLf & "Valor esperado: 58,33" & vbCrLf & "Valor obtido:" & Sheets(2).Range("F3").Value
            End
        End If
        
        If (Sheets(2).Range("G2").Value <> "1") Then 'Combinada
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Orçamento" & vbCrLf & "Valor esperado: 1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G2").Value
            End
        End If
        
        If (Sheets(2).Range("G3").Value <> "0") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & " do Orçamento" & vbCrLf & "Valor esperado: 0" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G3").Value
            End
        End If
        
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    
    Call LeftClick(504, 839) 'salvando
    Call LeftClick(688, 838) 'fechando
    
    
    '------------------- a partir daqui é a exportação do Orçamento
    
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(1072, 341) 'Gerar DAV
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "6102" 'Campo CFOP
    SendKeys "{ENTER}"
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "^(c)" 'Copiando o campo Forma de Pagamento para verificar
    Windows("Teste automatizado.xlsm").Activate
    Sheets("Dados para verificação").Select
    Range("A1").Select '--selecione a célula onde deseja colar
    ActiveSheet.Paste '-- código para colar
    
        If (Sheets(2).Range("A1").Value <> "5") Then 'Verificando se a forma de pagamento é 5
            MsgBox "Ocorreu algum erro com a forma de pagamento ao exportar o Orçamento" & vbCrLf & "Valor esperado: 5" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A1").Value
            End
        End If
        
    Call LeftClick(588, 415) 'Selecionando o produto 1
    Call LeftClick(588, 435) 'selecionando o serviço 1
    
    Call LeftClick(605, 762) 'Clicar no botão "Exportar"
    
    Application.Wait (Now + TimeValue("0:00:02"))
    
    Call RightClick(665, 423)
    SendKeys ("T") 'copiando a grade de produto
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos produtos no DAV
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
        If (Sheets(2).Range("D2").Value <> "1") Then 'Quantidade
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do DAV gerado pelo Orçamento" & vbCrLf & "Valor esperado: 1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D2").Value
            End
        End If
        
        If (Sheets(2).Range("D3").Value <> "4") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("D1").Value & " do DAV gerado pelo Orçamento" & vbCrLf & "Valor esperado: 4" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D3").Value
            End
        End If
        
        If (Sheets(2).Range("L2").Value <> "100") Then 'total liquido
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " do DAV gerado pelo Orçamento" & vbCrLf & "Valor esperado: 100" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L2").Value
            End
        End If
        
        If (Sheets(2).Range("L3").Value <> "25") Then
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("L1").Value & " do DAV gerado pelo Orçamento" & vbCrLf & "Valor esperado: 25" & vbCrLf & "Valor obtido:" & Sheets(2).Range("L3").Value
            End
        End If
        
    '---- limpando os dados
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    
    SendKeys "{ESC}" 'saindo da tela do DAV
    Application.Wait (Now + TimeValue("0:00:02"))
        
        
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados do Orçamento
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar
        
    
        If (Sheets(2).Range("G2").Value <> "175") Then 'Situação
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("G1").Value & "do Orçamento" & vbCrLf & "Valor esperado: 175" & vbCrLf & "Valor obtido:" & Sheets(2).Range("G2").Value
            End
        End If
        
        If (Sheets(2).Range("H2").Value <> "Finalizado  ") Then 'Situação
            MsgBox "Ocorreu algum erro com " & Sheets(2).Range("H1").Value & "do Orçamento" & vbCrLf & "Valor esperado: Finalizado  " & vbCrLf & "Valor obtido:" & Sheets(2).Range("H2").Value
            End
        End If
        
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    
    SendKeys "{ESC}"
End Sub


Sub Condicional()
    Call LeftClick(208, 67) 'Buscando a tela de Condicional
    Call LeftClick(227, 123)
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(1097, 608)
    Call LeftClick(791, 682) 'confirmando o filtro
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys ("{F2}") 'abrindo a tela de inclusão da condicional
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "{ENTER 2}"
    SendKeys "1" 'Informando o funcionario
    SendKeys "{ENTER}"
    SendKeys "1" 'Informando a Forma de Pagamento
    SendKeys "{ENTER}"
    SendKeys "3" 'Informando o Cliente
    SendKeys "{ENTER 2}"
    
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{F2}") 'abrindo a tela de inclusão do tipo de condicional
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "Tipo Condicional"
    SendKeys "{ENTER 3}" 'salvando o tipo condicional
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "{ENTER}" 'selecionando o tipo condicional
    SendKeys "{ENTER 2}" 'salvando o cabeçalho da condicional
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'informando o primeiro produto
        SendKeys "+{TAB}" 'colocando o foco no campo Quantidade
        SendKeys "9" 'Campo Quantidade
        SendKeys "{ENTER}"
        SendKeys "1" 'Campo Produto
        SendKeys "{ENTER}"
        Application.Wait (Now + TimeValue("0:00:01"))
    'informando o segundo produto
        SendKeys "+{TAB}" 'colocando o foco no campo Quantidade
        SendKeys "11" 'Campo Quantidade
        SendKeys "{ENTER}"
        SendKeys "2" 'Campo Produto
        SendKeys "{ENTER}"
        Application.Wait (Now + TimeValue("0:00:01"))
    'fim da inclusão dos produtos
    
    '--- verificação de dados dos itens da Condicional
    Call CopiarDados(717, 489)
    
        Call VerificarDados("9", "G2", "Condicional") 'Quantidade
        Call VerificarDados("11", "G3", "Condicional")
        
        Call VerificarDados("3", "J2", "Condicional") 'unitario
        Call VerificarDados("20", "J3", "Condicional")
        
        Call VerificarDados("27", "K2", "Condicional") 'Total
        Call VerificarDados("220", "K3", "Condicional")
        
        Call VerificarDados("0", "M2", "Condicional") 'Desconto
        Call VerificarDados("0", "M3", "Condicional")
        
        Call VerificarDados("27", "N2", "Condicional") 'Total líquido
        Call VerificarDados("220", "N3", "Condicional")
        
    Call LimparDados 'Fim da verificação
    
    Call LeftClick(557, 771) 'salvando
    Call LeftClick(677, 780) 'fechando a tela
    
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys ("{F7}") 'abrindo a tela de Detalhamaneto da condicional
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '--- verificação de dados dos itens da Condicional na tela de condicional
    Call CopiarDados(717, 357)
    
        Call VerificarDados("9", "G2", "Detalhamento da Condicional") 'Pendente
        Call VerificarDados("11", "G3", "Detalhamento da Condicional")
        
    Sheets(2).Range("A1:Z12").Select
    Selection.ClearContents
    Selection.NumberFormat = ""
    
    Call LeftClick(622, 292) 'Clicando no botão Devolver
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'devolvendo o primeiro produto
        SendKeys "4" 'Campo Quantidade
        SendKeys "{ENTER}"
        SendKeys "1" 'Campo Produto
        SendKeys "{ENTER}"
        Application.Wait (Now + TimeValue("0:00:01"))
    'devolvendo o segundo produto
        SendKeys "+{TAB}" 'colocando o foco no campo Quantidade
        SendKeys "6" 'Campo Quantidade
        SendKeys "{ENTER}"
        SendKeys "2" 'Campo Produto
        SendKeys "{ENTER}"
        Application.Wait (Now + TimeValue("0:00:01"))
    'fim da devolução dos produtos
    
    SendKeys "{ESC}" 'saindo da tela de devolução
    Application.Wait (Now + TimeValue("0:00:01"))
    
    
    'verificando os dados da tela de Detalhamento
    Call CopiarDados(717, 357)
    
        Call VerificarDados("4", "E2", "Detalhamento da Condicional") 'Devolvido
        Call VerificarDados("6", "E3", "Detalhamento da Condicional")
        
        Call VerificarDados("5", "G2", "Detalhamento da Condicional") 'Pendente
        Call VerificarDados("5", "G3", "Detalhamento da Condicional")
        
    Call LimparDados 'Fim da verificação
    
    Call LeftClick(729, 294)
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "7102" 'Campo CFOP
    SendKeys "{ENTER}"
    SendKeys "4" 'Campo Forma de Pagamento
    SendKeys "{ENTER}"
    Call LeftClick(596, 763) 'Clicando no botão Exportar
    Application.Wait (Now + TimeValue("0:00:02"))
    
    
    'verificando os dados do DAV Exportado
    Call CopiarDados(747, 441)
        Call VerificarDados("5", "D2", "DAV exportado da Condicional") 'Quantidade
        Call VerificarDados("5", "D3", "Detalhamento da Condicional")
        
        Call VerificarDados("15", "L2", "DAV exportado da Condicional") 'Total Líquido
        Call VerificarDados("100", "L3", "DAV exportado da Condicional")
        
    Call LimparDados 'Fim da verificação
    
    SendKeys "{ESC}" 'saindo da tela de DAV
    Application.Wait (Now + TimeValue("0:00:01"))


    'verificando os dados da tela de Detalhamento
    Call CopiarDados(717, 357)
    
        Call VerificarDados("4", "E2", "Detalhamento da Condicional") 'Devolvido
        Call VerificarDados("6", "E3", "Detalhamento da Condicional")
        
        Call VerificarDados("5", "F2", "Detalhamento da Condicional") 'Vendida
        Call VerificarDados("5", "F3", "Detalhamento da Condicional")
        
        Call VerificarDados("0", "G2", "Detalhamento da Condicional") 'Pendente
        Call VerificarDados("0", "G3", "Detalhamento da Condicional")
        
    Call LimparDados 'Fim da verificação
    
    SendKeys "{ESC}" 'saindo da tela de Detalhamento
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'Verificando a condicional
    Call CopiarDados(700, 433)
    
        Call VerificarDados("247", "J2", "Condicional") 'Total Líquido
        
        Call VerificarDados("Finalizado", "N2", "Condicional") 'Situação
        
        Call VerificarDados("A Vista", "S2", "Detalhamento da Condicional") 'Forma de Pagamento
        
    Call LimparDados 'Fim da verificação
    
    SendKeys "{ESC}" 'saindo da tela da Condicional
    Application.Wait (Now + TimeValue("0:00:01"))
End Sub


Sub LancamentoCR()
    Call LeftClick(326, 66)
    Call LeftClick(35, 122) 'Acessando a tela de Contas a Receber
    Application.Wait (Now + TimeValue("0:00:01"))
    
    Call LeftClick(1170, 610)
    SendKeys "{ENTER 2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys "{F2}" 'abrindo a tela de CR
    Application.Wait (Now + TimeValue("0:00:01"))
    
    '------------------- primeiro CR
        SendKeys "1" 'Cliente
        SendKeys "{ENTER}"
        SendKeys "137" 'Conta Contábil
        SendKeys "{ENTER 5}"
        SendKeys "500" 'Valor
        SendKeys "{ENTER}"
        SendKeys "CRM1" 'N Documento
        SendKeys "{ENTER}"
        SendKeys "1" 'Forma de Cobrança
        SendKeys "{ENTER 5}"
        Application.Wait (Now + TimeValue("0:00:01"))
        
    Call LeftClick(636, 781)
    SendKeys "{F12}"
    SendKeys "Crm1" 'filtrando somente o CR incluido
    Application.Wait (Now + TimeValue("0:00:01"))
    
    'Verificando o CR incluido
    Call CopiarDados(677, 432)
    
        Call VerificarDados("500", "G2", "Contas Receber Avulso") 'Valor Aberto
        
        Call VerificarDados("Aberto    ", "K2", "Contas Receber Avulso") 'Situação
        
        Call VerificarDados("1", "R2", "Contas Receber Avulso") 'Código Cliente
        
    Call LimparDados 'Fim da verificação
    
    
    Call LeftClick(663, 372)
    Application.Wait (Now + TimeValue("0:00:01"))
    Call LeftClick(678, 396)
    
    '------------------- Segundo CR
        SendKeys "1" 'Cliente
        SendKeys "{ENTER 4}"
        SendKeys "Crm2" 'N Documento
        SendKeys "{ENTER}"
        SendKeys "1" 'Forma de Cobrança
        SendKeys "{ENTER 3}"
        SendKeys "1000" 'Valor 1 parcela
        SendKeys "{ENTER 2}"
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys "{ENTER 4}" 'informando a segunda parcela
        Application.Wait (Now + TimeValue("0:00:01"))
        SendKeys "{TAB 5}" 'Acessando a guia lançamentos
        Application.Wait (Now + TimeValue("0:00:01"))
        Call LeftClick(654, 474)
        SendKeys "137" 'Conta contábil
        SendKeys "{ENTER 3}"
        Application.Wait (Now + TimeValue("0:00:01"))
        
        SendKeys "{TAB 4}"
        SendKeys "{ENTER}" 'Salvando o CR
        Call LeftClick(779, 756) 'Saindo da tela de inclusão do CR
        Application.Wait (Now + TimeValue("0:00:01"))
        Call LeftClick(779, 756) 'Saindo da tela de inclusão do CR
    Application.Wait (Now + TimeValue("0:00:01"))
    
    
    Call LeftClick(636, 781)
    SendKeys "{F12}"
    SendKeys "Crm2" 'filtrando somente o CR incluido
    Application.Wait (Now + TimeValue("0:00:01"))
    'Verificando o CR incluido
    Call CopiarDados(677, 432)
    
        Call VerificarDados("1000", "G2", "Contas Receber Avulso") 'Valor Aberto
        Call VerificarDados("1000", "G3", "Contas Receber Avulso")
        
        Call VerificarDados("Aberto    ", "K2", "Contas Receber Avulso") 'Situação
        Call VerificarDados("Aberto    ", "K3", "Contas Receber Avulso")
        
        Call VerificarDados("1", "R2", "Contas Receber Avulso") 'Código Cliente
        Call VerificarDados("1", "R3", "Contas Receber Avulso")
    Call LimparDados 'Fim da verificação
    
    SendKeys "{ESC}"
    
End Sub


