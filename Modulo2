Sub CadastrarUnidade()
    Call LeftClick(90, 68) 'Buscando a tela de Unidade
    Call LeftClick(97, 123)
    Call LeftClick(136, 392)
    SendKeys "{F2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "Unidade"
    SendKeys "{ENTER}"
    SendKeys "UN"
    Call LeftClick(825, 576) 'salvando a primeira unidade (UN)
    
    Application.Wait (Now + TimeValue("0:00:03"))
    SendKeys "{F2}"
    Application.Wait (Now + TimeValue("0:00:01"))
    SendKeys "Decimal"
    SendKeys "{ENTER}"
    SendKeys "DC"
    SendKeys "{ENTER}"
    SendKeys "{UP}"
    SendKeys "{ENTER 2}" 'salvando da segunda unidade (DC)
    
    
    Application.Wait (Now + TimeValue("0:00:03"))
    Application.CutCopyMode = False '--código para tirar a seleção de cópia (ganho na memória da máquina)
    Call RightClick(717, 431)
    SendKeys ("T") 'copia os dados para verificação
    Application.Wait (Now + TimeValue("0:00:01"))
    
    SendKeys "{ESC}" 'fechando a tela
    
    

    '--- verificação de dados
        Windows("Teste automatizado.xlsm").Activate
        Sheets("Dados para verificação").Select
        Range("A1").Select '--selecione a célula onde deseja colar
        ActiveSheet.Paste '-- código para colar

        If (Sheets(2).Range("A2").Value <> "Decimal") Then
            MsgBox "Ocorreu algum erro com a descrição da Unidade" & vbCrLf & "Valor esperado: Decimal" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A2").Value
            End
        End If
        If (Sheets(2).Range("B2").Value <> "2") Then
            MsgBox "Ocorreu algum erro com o código interno da Unidade" & vbCrLf & "Valor esperado: 2" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B2").Value
            End
        End If
        If (Sheets(2).Range("C2").Value <> "DC") Then
            MsgBox "Ocorreu algum erro com o simbolo da Unidade" & vbCrLf & "Valor esperado: DC" & vbCrLf & "Valor obtido:" & Sheets(2).Range("C2").Value
            End
        End If
        If (Sheets(2).Range("D2").Value <> "S") Then
            MsgBox "Ocorreu algum erro com o Ativo/Inativo da Unidade" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D2").Value
            End
        End If
        '----Verificação da outra Unidade

        If (Sheets(2).Range("A3").Value <> "Unidade") Then
            MsgBox "Ocorreu algum erro com a descrição da Unidade" & vbCrLf & "Valor esperado: Unidade" & vbCrLf & "Valor obtido:" & Sheets(2).Range("A3").Value
            End
        End If
        If (Sheets(2).Range("B3").Value <> "1") Then
            MsgBox "Ocorreu algum erro com o código interno da Unidade" & vbCrLf & "Valor esperado: 1" & vbCrLf & "Valor obtido:" & Sheets(2).Range("B3").Value
            End
        End If
        If (Sheets(2).Range("C3").Value <> "UN") Then
            MsgBox "Ocorreu algum erro com o simbolo da Unidade" & vbCrLf & "Valor esperado: UN" & vbCrLf & "Valor obtido:" & Sheets(2).Range("C3").Value
            End
        End If
        If (Sheets(2).Range("D3").Value <> "S") Then
            MsgBox "Ocorreu algum erro com o Ativo/Inativo da Unidade" & vbCrLf & "Valor esperado: S" & vbCrLf & "Valor obtido:" & Sheets(2).Range("D3").Value
            End
        End If

    '---- fim da verificação de dados
    
    '---- limpando os dados
        Sheets(2).Range("A1:Z12").Select
        Selection.ClearContents
        Selection.NumberFormat = ""
End Sub




